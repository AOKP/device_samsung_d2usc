# system.prop for SCH-R530U
#

# cannot take spaces
rild.libargs=-d /dev/smd0

ro.cdma.home.operator.numeric=311580
ro.cdma.home.operator.alpha=U.S.Cellular
ro.cdma.subscribe_on_ruim_ready=true
ro.ril.hsxpa=1
ro.ril.gprsclass=10
ro.telephony.default_network=8
keyguard.no_require_sim=true
DEVICE_PROVISIONED=1
telephony.lteOnCdmaDevice=1
ro.config.svlte1x=true
persist.radio.no_wait_for_card=0
persist.radio.snapshot_enabled=1
persist.radio.snapshot_timer=22
ro.config.combined_signal=true
ro.gsm.data_retry_config=max_retries=infinite,0,0,60000,120000,480000,900000
persist.eons.enabled=false

# hardware info
ro.device.chipset=Qualcomm Snapdragon S4 MSM8960
ro.device.cpu=1.5 GHz dual-core Krait
ro.device.gpu=Qualcomm Adreno 225
ro.device.rear_cam=8 MP
ro.device.front_cam=1.9 MP
ro.device.screen_res=720x1280
